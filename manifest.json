{
    "name": "UST space AD remover",
    "description": "Base Level Extension",
    "version": "1.1.1",
    "manifest_version": 3,
    "action": {
      "default_icon": "remover.png"
    },
    "background": {
      "service_worker": "service_worker.js"
    },
    "permissions": [
      "tabs",
      "scripting",
      "webRequest"
    ],
    "host_permissions": [
      "https://ust.space/*"
    ]
  }